                    else
                    {
                        [string]('{0:N2}' -f ($_.FreeSpace/1mb))+"Mb"
                    }
					</ScriptBlock>
					</TableColumnItem>
                </TableColumnItems>
              </TableRowEntry>
            </TableRowEntries>
          </TableControl>
        </View>       
