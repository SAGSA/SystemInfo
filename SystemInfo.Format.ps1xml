                <Alignment>left</Alignment>
              </TableColumnHeader>
              <TableColumnHeader>
                <Label>SmartStatus</Label>
                <Alignment>left</Alignment>
              </TableColumnHeader>
            </TableHeaders>
            <TableRowEntries>
              <TableRowEntry>
                <TableColumnItems>
                  <TableColumnItem>
                    <ScriptBlock>
                    if ($_.Size -ge 1gb)
                    {
                        [string]('{0:N0}' -f ($_.Size/1gb))+"Gb"
                    }
                    else
                    {
                        [string]('{0:N0}' -f ($_.Size/1mb))+"Mb"
                    }
                    </ScriptBlock>
                  </TableColumnItem>
                   <TableColumnItem>
                    <ScriptBlock>$_.interfacetype</ScriptBlock>
                  </TableColumnItem>
                  <TableColumnItem>
                    <PropertyName>Model</PropertyName>
                  </TableColumnItem>
				  <TableColumnItem>
                    <PropertyName>Type</PropertyName>
                  </TableColumnItem>
                  <TableColumnItem>
                    <PropertyName>SmartStatus</PropertyName>
