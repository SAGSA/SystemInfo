		<View>
            <Name>Default</Name>
            <ViewSelectedBy>
                <TypeName>ModuleSystemInfo.Systeminfo.Hdd.Devices</TypeName>
            </ViewSelectedBy>
            <TableControl>
            <TableHeaders>
              <TableColumnHeader>
                <Label>Size</Label>
                <Alignment>left</Alignment>
